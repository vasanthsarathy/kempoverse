{
  "name": "kempoverse",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:wrangler": "wrangler pages dev dist --d1=DB=kempoverse-db",
    "dev:full": "npm run build && npm run dev:wrangler",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "db:create": "wrangler d1 create kempoverse-db",
    "db:migrate:local": "wrangler d1 migrations apply kempoverse-db --local",
    "db:migrate": "wrangler d1 migrations apply kempoverse-db --remote",
    "db:seed:local": "wrangler d1 execute kempoverse-db --local --file=./migrations/seed.sql",
    "db:seed": "wrangler d1 execute kempoverse-db --remote --file=./migrations/seed.sql",
    "db:query:local": "wrangler d1 execute kempoverse-db --local --command",
    "db:query": "wrangler d1 execute kempoverse-db --remote --command",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && wrangler pages deploy dist"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251205.0",
    "@eslint/js": "^9.17.0",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^5.4.11",
    "wrangler": "^4.53.0"
  }
}
